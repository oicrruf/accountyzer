<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\ServiceDetail" table="service_detail">
    <indexes>
      <index name="fk_service_detail_service_idx" columns="service"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="id_UNIQUE" columns="id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id"/>
    <id name="service" association-key="true"/>
    <field name="name" type="string" column="name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <one-to-one field="service" target-entity="Service" fetch="LAZY">
      <join-columns>
        <join-column name="service" referenced-column-name="id"/>
      </join-columns>
    </one-to-one>
  </entity>
</doctrine-mapping>
