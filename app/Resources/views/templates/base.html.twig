<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Inicio | Accountyzer {% endblock %}</title>
        {% block stylesheets %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <link href="{{ asset('build/common.css') }}" rel="stylesheet" />
        <link href="{{ asset('build/accountyzer.css') }}" rel="stylesheet" />
    </head>
    <body>
        {% block navbar %}
            {{ include('templates/navbar.html.twig')}}
        {% endblock %}
        {% block body %}{% endblock %}
        {% block footer %}
            {{ include('templates/footer.html.twig')}}
        {% endblock %}
        {% block javascripts %}
        <script src="{{ asset('build/common.js') }}"></script>
        {% endblock %}
    </body>
</html>
